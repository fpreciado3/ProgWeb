<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Person: <%= ID %></h1>
    <h2>Mensaje: <%= MSG %></h2>
    <% switch(MSG) {  %>
        <%  case 'Hola': %>
            <% if(<%= TMS %> > 0) { %>
               <% for(let i = 1; i <= <%= TMS %>; i++) { %>
                    <h3>
                        <%= MSG %> 
                        <%= ID %>
                    </h3>
               <% } %>
           <% } %>
         <% break; %>
        <% case 'Bienvenido': %>
        <% if(<%= TMS %> > 0) { %>
            <% for(let i = 1; i <= <%= TMS %>; i++) { %>
                 <h3>
                    <%= MSG %> 
                    <%= ID %>
                 </h3>
            <% } %>
        <% } %>
      <% break; %>
      <%  case 'Adios': %>
        <% if(<%= TMS %> > 0) { %>
            <% for(let i = 1; i <= <%= TMS %>; i++) { %>
                 <h3>
                    <%= MSG %> 
                    <%= ID %>
                </h3>
            <% } %>
        <% } %>
      <% break; %>
        <%  default: %>
  <%  } %>
</body>
</html>